from eagle.task.models import *
from eagle.account.models import *

u1 = Account.objects.get(pk=1)

t1 = Tag.objects.get(name='atag1')
t2 = Tag.objects.create(name='atag2')


task1 = Task.objects.create(title='test 1 ', detail='this is the detail part of test 1', priority=2, mode = 1, owner=u1, tag = t1)

task2 = Task.objects.create(title='test 2 ', detail='this is the detail part of test 2', priority=2, mode = 2, owner=u1, tag = t1)

task3 = Task.objects.create(title='test 3 ', detail='this is the detail part of test  3', priority=2, mode = 3, owner=u1, tag = t2)

t1.save()
t2.save()
task1.save()
task2.save()
task3.save()
